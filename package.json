{
  "name": "rpx-exui",
  "version": "0.0.0",
  "engines": {
    "yarn": "^1.12.3",
    "npm": "^5.6.0"
  },
  "scripts": {
    "nuke": "rm -rf dist",
    "build": "yarn build:prod && yarn build:node",
    "pre-push": "npm run lint && npm run build:prod && ng test --code-coverage",
    "start": "node ./dist/rpx-exui/api/server.js",
    "build:node": "cd api && yarn build",
    "build:prod": "ng build --prod",
    "start:node": "cd api && npm run watch",
    "lint:node": "cd api && npm run lint",
    "test:node": "cd api && npm run test",
    "coverage:node": "cd api && npm run coverage",
    "ng": "ng",
    "start:ng": "ng serve --port 3000 --host 0.0.0.0 --proxy-config proxy.config.json",
    "build:ng": "ng build",
    "build:docker": "npm run build:prod && docker build -t xui .",
    "lint": "npm run lint:ng && npm run lint:node",
    "test": "cd api && npm run coverage && cd .. && npm run test:ng",
    "test:ng": "ng test",
    "lint:ng": "ng lint",
    "e2e": "ng e2e",
    "test:coverage": "echo 'not implemented'",
    "test:coverage:merge": "./node_modules/.bin/lcov-result-merger './reports/tests/coverage/*/lcov.info' > ./reports/tests/coverage/lcov.info",
    "test:coverage:node": "cd api && npm run coverage",
    "test:coverage:ng": "npm run test:ng --source-map --code-coverage",
    "test:a11y": "echo 'not implemented'",
    "test:all": "npm run test:ng && npm run test:node && npm run test:smoke && npm run test:functional && npm run test:crossbrowser",
    "test:smoke": "sh whitelist.sh && webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/smoke.conf.js --local",
    "test:smoke:local": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/smoke.conf.js --local",
    "test:functional": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/conf.js --local",
    "test:functional:local": "protractor ./test/e2e/config/functional.conf.js --local",
    "test:crossbrowser": "protractor ./test/e2e/config/crossbrowser.conf.js",
    "test:fullfunctional": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/conf.js --local",
    "test:mutation": "echo 'not implemented'",
    "test:apiGateway": "echo 'not implemented'",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test-pact": "NODE_PATH=. LOG_LEVEL=error mocha --timeout 10000 -r ts-node/register api/test/pact/pact-tests/*.ts",
    "publish-pact": "NODE_PATH=. LOG_LEVEL=error mocha 'api/test/pact/publish/*.js' --timeout 15000 --reporter spec",
    "test:pact:run-and-publish": "yarn test-pact && yarn publish-pact"
  },
  "private": true,
