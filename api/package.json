{
    "name": "jui",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "build": "tsc",
        "coverage": "npx nyc mocha",
        "lint": "npx tslint --project .",
        "watch": "nodemon --watch '**/*.ts' --exec 'ts-node' local.ts",
        "test": "mocha || true",
        "test:watch": "mocha --watch --recursive --watch-extensions ts",
        "start": "cd ../dist/jui-backend && node server.js"
    },
    "private": true,
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "lines": 0,
        "statements": 0,
        "functions": 0,
        "branches": 0,
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "controllers",
            "lib",
            "services"
        ],
        "exclude": [
            "**/*.d.ts",
            "**/*.spec.ts",
            "**/templates"
        ],
        "reporter": [
            "lcov",
            "html",
            "text-summary"
        ],
        "require": [],
        "cache": true,
        "all": true,
        "report-dir": "../reports/tests/coverage/node"
    },
    "dependencies": {}
}
